	month = sep,
	year = {2014},
	note = {Loc: Paris, France;},
	file = {Hennequet_2014_L'identité poétique de la nation.pdf:/Users/elotroalex/Dropbox/zotero-files/Hennequet_2014_L'identité poétique de la nation.pdf:application/pdf}

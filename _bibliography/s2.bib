@phdthesis{kaufman_maximin_1997,
	type = {Doctoral},
	title = {Maximin, {Cixous}, and {Césaire}: {Creolization}, {Intertextuality}, and {Coiled} {Myth}},
	shorttitle = {Maximin, {Cixous}, and {Césaire}},
	url = {http://proquest.umi.com/login},
	abstract = {In his postmodern novel 'l'Isole soleil,' Guadeloupean Daniel Maximin is perhaps unique among French West Indian authors in his validation of black Antillean women. Contrary to the practice of creolistes Jean Bernabé, Patrick Chamoiseau, and Raphael Confiant, whose works exclude women from cultural transmission and production, Maximin's female characters are his principal narrators and his protagonist Marie-Gabriel is writing a novel of her own. Using French feminist Hélène Cixous's techniques of writing the body, Maximin nevertheless subtly corrects her essentialism by extending her metaphors for feminine healing, empowerment, and transcendence to men. His writing techniques evoking gestations, births, connections, and diffused boundaries suggest the permeability of sexual, racial, and class barriers. His ideology of creolization, similar to that expressed by Édouard Glissant's in Introduction a une poétique du divers (1946), advocates the bringing together of the most disparate cultural elements in a context of intervalorization with unpredictable results. Maximin writes explosively by juxtaposing intertexual borrowings from Aimé Césaire, Hélène Cixous, and other authors of multiple historical, social, and cultural perspectives to encourage inclusiveness and the acceptance of conflict. Imitating Cixous's spiral narrative structures, he creates patterns of cyclic destructiveness to condemn rash and violent solutions to complex interpersonal, interracial, or international problems. His literary experimentation with open-endedness fosters dialogue. The author counters the negative and predominantly occidental stereotypes of traditional Antillean literature, which portrayed the colonized islands as "feminine and diseased," by rewriting Guadeloupean history to affirm both Antillean women and men. As Aimé Césaire originated a new mythology for Martinican blacks and Hélène Cixous posited an original unified world ruled by Demeter, Maximin's protagonist Marie-Gabriel heals herself and her people by writing a new creation myth that justifies their existence in the New World.},
	language = {en},
	school = {University of Virginia},
	author = {Kaufman, Janice Horner},
	year = {1997},
	note = {Loc: Charlottesville-VA, United States;},
	file = {Kaufman_1997_Maximin, Cixous, and Césaire.pdf:/Users/ag3339/Dropbox/zotero-files/Kaufman_1997_Maximin, Cixous, and Césaire.pdf:application/pdf}
}

@phdthesis{buxton_uses_1997,
	address = {England},
	type = {Ph.{D}.},
	title = {Uses of the surrealist voice: a comparative study of the poetry of {Aimé} {Césaire} and {Octavio} {Paz}},
	copyright = {Copyright ProQuest, UMI Dissertations Publishing 1997},
	shorttitle = {Uses of the surrealist voice},
	url = {http://search.proquest.com.ezproxy.cul.columbia.edu/docview/301473999/1422923E35040BA4003/312?accountid=10226},
	abstract = {This thesis examines the influences of Surrealism on Aimé Césaire and Octavio Paz and explores how the Latin American allegiances of the poets affect these influences. This comparison is instructive for its illumination of both the unity of Latin American culture and the relationship between this culture and the European avant-garde. The thesis focuses upon three issues basic to Surrealism: language, identity, and the 'primitive'.After a survey of the direct contacts Paz and Césaire had with the Parisian Surrealists, an examination is made of the poets' attitude to language, revealing that they share the Surrealists' faith in the magical power of the word. This faith, when related to Surrealism's ambitions for social change, is particularly relevant to the poets in the context of the specific history of colonisation and its effects in Latin America. Surrealism allows the poets to experiment freely with language, seeking out their own form of expression. It also combats what Paz and Césaire view as an inferiority complex among the Mexicans and Martinicans by re-evaluating the pre-Columbian and African elements in Latin American culture which, according to the poets, have been repressed in favour of European ones.Surrealism encourages the poets to seek a collective identity based on these non-European aspects of Latin American culture which are purportedly buried in a shared unconscious relating to images of the pre-colonial or African past of the Mexicans and Martinicans. In the search of this aspect of identity, Paz and Césaire's poetic imagery evokes the Surrealist ideal of Breton's 'point supreme'. However, this ideal of a more authentic identity is problematical for Césaire and Paz, and as they mature, their belief in attaining a stable or fixed form of self diminishes.},
	language = {en},
	urldate = {2013-12-05},
	school = {The University of Manchester (United Kingdom)},
	author = {Buxton, E.},
	year = {1997},
	note = {Loc: Manchester, England;}
}


	month = may,
	year = {2013},
	note = {Loc: Paris, France;},
	keywords = {botanique, botany, Césaire, herbarium, herbier, imaginaire, landscape, Martinique, nature, paysage},
	file = {Constans_2013_« Essentiel paysage ».pdf:/Users/alex/Zotero/storage/IPCW5QK2/Constans_2013_« Essentiel paysage ».pdf:application/pdf;Snapshot:/Users/alex/Zotero/storage/ATSMETVV/25910.html:text/html}

@phdthesis{desroches_espace_2000,
	type = {Doctoral},
	title = {L'{Espace} rapaillé},
	abstract = {Gaston Miron (1928-1996) played a central role in a drastic shift in Quebecois identity around 1960. Through close textual readings of his collection of poems, L'Homme rapaillé (1970), I analyze representations of space in his work, as well as in those of Caribbean poets Aimé Césaire and Edouard Glissant.

The study reveals the metaphorical systems of spatial representations underlying Miron's thematics of alienation, love, death and the country as an entity yet to be realized. At the heart of Miron's conception of space lies the quest for the horizon. Using Michel Collot's concept of horizon structure, I show a symbolic correspondence between horizon and language, which I find in the works of Miron, Césaire and Glissant. The relationship of language and literature being a characteristic of postcolonial literatures, the study draws conclusions about the pertinence of the postcolonial critical discourse for further studies on Quebec literature. Finally, I argue for a re-evaluation of Miron in the francophone corpus as well as in the corpus of Quebec literary works studied in United States and English Canada. A complete concordance of L'Homme rapaillé is included as an appendix.},
	language = {fr},
	school = {Columbia University},
	author = {DesRoches, Vincent},
	year = {2000},
	note = {Loc: New York-NY, United States;}
}

@phdthesis{dahouda_aime_2000,
	address = {Canada},
	type = {Ph.{D}.},
	title = {Aimé {Césaire}, {Paul} {Chamberland} et les pays imaginaire},
	copyright = {Copyright UMI - Dissertations Publishing 2000},
	url = {http://search.proquest.com.ezproxy.cul.columbia.edu/docview/304656472/abstract/1422923E35040BA4003/120?accountid=10226},
	abstract = {Cette thèse analyse des figures du pays et leurs fonctions sociales dans les poésies de Césaire et de Chamberland.
Son objectif est circonscrit autour de six recueils de poésie. Pour Césaire, il s'agit du Cahier d'un retour au pays natal , de Ferrements , et de moi, laminaire ... Pour Paul Chamberland, il s'agit de Terre Québec , de L'afficheur hurle , et de L'inavouable .
Suivant des modalités propres, ces textes représentent le pays selon deux grands axes: un axe négatif, où le pays épouse une vision de la violence, puis incarne une figure de l'exil. Un axe positif, où le mal d'être du pays détermine une subversion par laquelle les deux poètes tentent de redéfinir la vision du pays natal. Désir de renouveau qui rejoint une volonté collective de changement à l'oeuvre dans le discours social, aux Antilles et au Québec.
Abordée selon la méthode de la sociopoétique comparée, la recherche contribue à la connaissance des relations que les poésies de Césaire et de Chamberland entretiennent dans l'investissement social de la figure du pays.},
	language = {fr},
	urldate = {2013-12-05},
	school = {Université Laval (Canada)},
	author = {Dahouda, Kanaté},
	year = {2000},
	note = {Loc: Québec, Canada;}
}


@phdthesis{kubayanda_nicolas_1982,
	address = {United States -- Missouri},
	type = {Ph.{D}.},
	title = {Nicolás {Guillén} and {Aimé} {Césaire}: {A} '{Universalist}' {Approach} to the {Poetics} of {Africanness} in {Latin} {America} and the {Caribbean}, 1929-1961},
	copyright = {Copyright UMI - Dissertations Publishing 1981},
	shorttitle = {Nicolás {Guillén} and {Aimé} {Césaire}},
	url = {http://search.proquest.com.ezproxy.cul.columbia.edu/docview/303180685/1422923E35040BA4003/330?accountid=10226},
	abstract = {The primary objective of this dissertation is the application of the "universalist method" to the poetry of Guillen and Cesaire. This "method" advocates the study of literary works across national boundaries, linguistic barriers, and several disciplines. Accordingly, the poetic texts of Guillen and Cesaire are examined as an indivisible whole, coextensive with the Africanist poetics that came to the fore contemporaneously in the Caribbean and Latin-American mainland from the 1920's. The dividing line between Hispanophone negrismo and Francophone Negritude is thus destroyed; the largely common attitudes on the part of both poets toward anthropology, history, language, and culture are illustrated.
It is established that Guillen and Cesaire reformulate with similar passion and emphasis the images of African Man and things African prevalent in scientific and popular thought in Latin America and the rest of the West. It is further shown that the African Man functions in the literary world of Guillen and Cesaire as a reflection of the socio-historical conditions in the New World and Africa. It is also demonstrated that the Spanish and French languages emerge in both poets as instruments of a powerful identity, as well as tools of transformation in consciousness. In creative display is a new poetic language based on the African-American oral forms. Finally, it is argued, with multiple examples, that the poetry of Guillen and Cesaire embodies a complex aesthetic of African polyrhythms never before experienced in Latin-American written poetry, and only improperly discussed in previous studies.
In light of the above, it is submitted in this study that Guillen and Cesaire have left a permanent mark in Latin-American and Caribbean letters, and that the poetics of Africanness can no longer be viewed as a peripheral and fragmented mode, but as a coherent, ongoing creative activity.},
	language = {en},
	school = {Washington University},
	author = {Kubayanda, Josaphat Bekunuru},
	year = {1982},
	note = {Loc: Worcester, Ma, United States;}
}


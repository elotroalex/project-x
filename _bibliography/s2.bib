@article{calixte_aime_2008,
	title = {Aimé {Césaire}, {Jacques} {Roumain} et l'équivoque du « retour »},
	volume = {77},
	abstract = {Cet article aborde la question du «retour» chez Aimé Césaire et Jacques Roumain. Ces deux écrivains, comme d'autres romanciers et poètes caribéens, ont la particularité d'actualiser le vieux rêve de retour en terre natale des esclaves transplantés durant la période coloniale dans le Nouveau Monde. Ainsi, ils reproduisent par la fiction les malaises hérités de la société coloniale. Mais les auteurs qui renouvèlent cette thématique le font généralement sous la forme d'une recherche obsessionnelle d'une vie idéale à réaliser hors de leur terre natale. Jacques Roumain se situe dans cette tradition bien qu'en apportant quelques ajouts. Quant à Aimé Césaire, il propose dans son Cahier d'un retour au pays natal une analyse du désir du retour, examine ses raisons sous-jacentes et tente d'instituer le sujet antillais en maître et artisan de son présent. Dans les lignes qui suivent, nous confrontons ces deux visions particulières du retour.},
	language = {fr},
	journal = {Présence francophone},
	author = {Calixte, Fritz},
	month = may,
	year = {2008},
	note = {Loc: Sherbrooke-QC, Canada;},
	pages = {146--147}
}

